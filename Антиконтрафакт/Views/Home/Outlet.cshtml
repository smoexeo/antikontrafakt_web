@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <title>Информация об организации</title>
</head>
<body>
    <header>
        <nav class="navbar navbar-light bg-light justify-content-between">
            <a href="/Home/Index" class="navbar-brand" style="color:#F3533B;">АНТИКонтрафакт</a>
            <span style="margin-right: 10px;">
                <a href="/Home/Index">
                    <img src="~/HTML-Страницы/merge/person-circle.svg">
                </a>
            </span>
        </nav>
    </header>

    <div>
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-0">
                    <span style="
					font-family: Roboto;
					font-style: normal;
					font-weight: normal;
					font-size: 32px;
					line-height: 49px;
					display: flex;
					align-items: center;
					text-align: center;
					color: #F3533B;">
                        Информация об организации
                    </span>
                </div>

            </div>
        </div>

        <div style="">
            <div class="container " style="  padding-top: 40px; padding-bottom: 40px; ">
                <div class="row justify-content-center">
                    <div class=" col-12">
                        <div class="form-group">
                            <label>ИНН:</label>
                            <label>@{ @ViewBag.Tin; }</label>
                        </div>
                        <div class="form-group">
                            <label> Зарегестрирован в реестре:</label>
                            <label> @{ @ViewBag.Good; }</label>
                        </div>
                        <div class="form-group">
                            <label> Название организации:</label>
                            <label> @{ @ViewBag.Name; } </label>
                        </div>
                        <div class="form-group">
                            <label> Адрес:</label>
                            <label>	@{ @ViewBag.Address; } </label>
                        </div>
                        <div class="form-group">
                        </div>

                        <div class="form-group">
                        </div>


                    </div>
                </div>
            </div>
        </div>
</body>
</html>
