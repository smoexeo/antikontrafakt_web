@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <title>Информация об организации</title>
</head>
<body>
    <header>
        <nav class="navbar navbar-light bg-light justify-content-between">
            <a href="/Home/Index" class="navbar-brand" style="color:#F3533B;">АНТИКонтрафакт</a>
            <span style="margin-right: 10px;">
                <a href="/Home/Account">
                    <img src="~/HTML-Страницы/merge/person-circle.svg">
                </a>
            </span>
        </nav>
    </header>


    <h1>ИНФОРМАЦИЯ ОБ ОРГАНИЗАЦИИ</h1>
    <div class="container-fluid">
        <div class="row">
            <div class=" col-12">
                <div class="form-group">
                    <h3>
                        ИНН:
                        @{ @ViewBag.Tin; }
                    </h3>
                </div>
                <div class="form-group">
                    <label> Зарегестрирован в реестре:</label>
                    <label>@{ @ViewBag.Good; } </label>
                </div>
                <div class="form-group">
                    <label> Название организации:</label>
                    <label> @{ @ViewBag.Name; } </label>
                </div>
                <div class="form-group">
                    <label> Адрес:</label>
                    <label>	@{ @ViewBag.Address; } </label>
                </div>
                

            </div>
        </div>
    </div>
    
</body>
</html>
